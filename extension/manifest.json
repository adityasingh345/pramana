{
  "manifest_version": 3,
  "name": "iSafe Analyzer",
  "version": "1.0",

  "permissions": ["activeTab", "scripting"],

  "host_permissions": ["<all_urls>"],

  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
